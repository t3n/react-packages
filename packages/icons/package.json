{
  "name": "@t3n/icons",
  "version": "0.4.5",
  "description": "t3n.de react icons",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/t3n/react-packages.git"
  },
  "main": "dist/bundle.js",
  "types": "src/components/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist && rm -rf .awcache && rm -rf src/components",
    "check-types": "tsc",
    "start": "webpack --watch",
    "build": "npm run clean && npm run generate && NODE_ENV=production webpack",
    "start:generate": "ts-node-dev -P tsconfig.node.json --respawn --transpile-only src/lib/generate.ts",
    "generate": "ts-node-dev -P tsconfig.node.json --files src/lib/generate.ts"
  },
  "peerDependencies": {
    "react": ">=16.10"
  },
  "devDependencies": {
    "@svgr/core": "^5.0.1",
    "@svgr/plugin-jsx": "^5.0.1",
    "@svgr/plugin-prettier": "^5.0.1",
    "@svgr/plugin-svgo": "^5.0.1",
    "@t3n/configs": "^0.4.4",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.14.10",
    "@types/prettier": "^2.0.1",
    "@types/react": "^17.0.0",
    "chalk": "^4.1.0",
    "fs-extra": "^9.0.1",
    "material-design-icons": "^3.0.1",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^4.1.2"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
