{
  "name": "@t3n/components",
  "version": "0.2.54",
  "description": "t3n react components",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/t3n/react-packages.git"
  },
  "main": "dist/bundle.js",
  "types": "src/index.d.ts",
  "dependencies": {
    "@types/btoa": "^1.2.3",
    "@types/react-autosuggest": "^9.3.13",
    "btoa": "^1.2.1",
    "framer-motion": "^1.6.17",
    "polished": "^3.4.1",
    "react-autosuggest": "^9.4.3",
    "react-imgix": "^9.0.1",
    "react-spring": "^8.0.27",
    "react-use-scroll-position": "github:neo/react-use-scroll-position",
    "styled-system": "^5.1.4",
    "toasted-notes": "^3.0.0",
    "use-debounce": "^3.2.0"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist && rm -rf .awcache",
    "check-types": "tsc",
    "start": "webpack --watch",
    "build": "NODE_ENV=production webpack",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "devDependencies": {
    "@t3n/configs": "^0.2.11",
    "@t3n/icons": "^0.2.18",
    "@t3n/theme": "^0.2.18",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.3",
    "@types/styled-components": "^4.4.0",
    "@types/styled-system": "^5.1.4",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.4",
    "jsdom": "^15.2.0",
    "jsdom-global": "^3.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "styled-components": "^4.4.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "@t3n/icons": ">=0.2",
    "@t3n/theme": ">=0.2",
    "react": ">=16.10",
    "react-dom": ">=16.10",
    "styled-components": ">=4"
  },
  "jest": {
    "displayName": "components",
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "testEnvironment": "node",
    "testRegex": [
      "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>/setupFile.js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
