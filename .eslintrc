{
  "root": true,
  "rules": {
    "import/extensions": 0,
    "no-use-before-define": 0,
    "react/default-props-match-prop-types": 0,
    "react/function-component-definition": [
      2,
      { "namedComponents": "arrow-function" }
    ]
  },
  "extends": [
    "@t3n/eslint-config",
    "@t3n/eslint-config-react",
    "@t3n/eslint-config-typescript"
  ]
}
